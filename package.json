{
  "name": "a1crmcatalyst",
  "version": "1.0.0",
  "description": "A1 CRM Catalyst - Accelerating Your Customer Growth with Smart CRM Solutions",
  "homepage": "https://skamanulla.github.io/A1CRMCatalyst",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-mysql": "concurrently \"npm run server-mysql\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "server-mysql": "cd backend && npm run dev-mysql",
    "client": "cd frontend && npm start",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "crm",
    "catalyst",
    "customer-growth",
    "salesforce",
    "react",
    "node",
    "mysql"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "gh-pages": "^6.3.0"
  }
}
